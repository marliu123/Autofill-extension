{
    "name": "Autofiller",
    "description": "An autofiller for a website",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
          "matches": ["https://*/*"],
          "js": ["content.js"]
        }
    ],
    "permissions": ["activeTab"]
  }

  